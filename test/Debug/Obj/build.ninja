#Generating source browse information for project test simple


#Abbreviations
cc = C$:\Program$ Files\IAR$ Systems\Embedded$ Workbench$ 8.5\common\bin\SourceIndexer.exe
ll = C$:\Program$ Files\IAR$ Systems\Embedded$ Workbench$ 8.5\common\bin\PbdLink.exe
bd = C$:\Program$ Files\IAR$ Systems\Embedded$ Workbench$ 8.5\common\bin\makeBrowseData.exe



#Rules
rule index
  depfile = $out.dep
  command = $cc -out=$out -f $in
rule link
  command = $ll -M $out $in
rule browsedata
  command = $bd $in -output  $out



#Build steps
build adc.pbi : index adc.xcl
build clocks.pbi : index clocks.xcl
build detectors.pbi : index detectors.xcl
build init.pbi : index init.xcl
build iot.pbi : index iot.xcl
build led.pbi : index led.xcl
build main.pbi : index main.xcl
build menu.pbi : index menu.xcl
build pid.pbi : index pid.xcl
build ports.pbi : index ports.xcl
build serial.pbi : index serial.xcl
build sm.pbi : index sm.xcl
build switches.pbi : index switches.xcl
build system.pbi : index system.xcl
build timers.pbi : index timers.xcl
build utils.pbi : index utils.xcl
build wheels.pbi : index wheels.xcl
build test$ simple_part0.pbi : link adc.pbi clocks.pbi detectors.pbi init.pbi
build test$ simple_part1.pbi : link iot.pbi led.pbi main.pbi menu.pbi
build test$ simple_part2.pbi : link pid.pbi ports.pbi serial.pbi sm.pbi
build test$ simple_part3.pbi : link switches.pbi system.pbi timers.pbi utils.pbi
build test$ simple_part4.pbi : link wheels.pbi
build test$ simple.pbd : link test$ simple_part0.pbi test$ simple_part1.pbi test$ simple_part2.pbi test$ simple_part3.pbi test$ simple_part4.pbi
build test$ simple.pbw : browsedata test$ simple.pbd


